{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Distribution of Life Expectancy and Birth Rate by Country in 2023",
    "data": {
      "url": "world-data-2023 2.csv"
    },
    "vconcat": [
      {
        "mark": "boxplot",
        "encoding": {
          "x": {
            "field": "Life expectancy",
            "type": "quantitative",
            "title": "Life Expectancy"
          },
          "color": {
            "value": "#fc9272"
          }
        }
      },
      {
        "mark": "boxplot",
        "encoding": {
          "x": {
            "field": "Birth Rate",
            "type": "quantitative",
            "title": "Birth Rate"
          },
          "color": {
            "value": "#fee0d2"
          }
        }
      }
    ]
  }
  