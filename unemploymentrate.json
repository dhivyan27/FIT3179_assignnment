{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 150,
    "title": "Distribution of Life Expectancy by Country in 2023",
    "data": {
        "url": "world-data-2023 2.csv"
    },
    "mark": {
        "type": "boxplot",
        "color": "#fc9272",
        "extent": "min-max"
    },
    "encoding": {
        "x": {
            "field": "Life expectancy",
            "type": "quantitative",
            "title": "Life Expectancy",
            "scale": {
                "domain": [40, 100]
            }
        },
        "color": {
            "value": "#fee0d2"
        }
    }
}
